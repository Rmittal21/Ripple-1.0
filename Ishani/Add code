'''
class Solution {
public:
    bool isPalindrome(int x) {
        int dig=0;
        long int rev = 0;
        int temp = x;
        while(temp>0){
            dig = temp%10;
            rev= rev*10 + dig;
            temp/=10;
        }
        if(rev==x){
            return true;
        }
        else
            return false;
    }
};
'''
